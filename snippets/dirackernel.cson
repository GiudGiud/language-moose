# Template for DiracKernel

'.source.c, .source.cpp':
  'MOOSE Dirac kernel header':
    'prefix': 'dirac-h'
    'body': """
    #pragma once

    #include "DiracKernel.h"

    class ${1};

    template <>
    InputParameters
    validParams<${1}>();

    /**
     *
     */
    class ${1} : public DiracKernel
    {
    public:
      ${1}(const InputParameters & parameters);

      virtual void addPoints() override;
      virtual Real computeQpResidual() override;
      virtual Real computeQpJacobian() override;
    };
    """


  'MOOSE Dirac kernel implementation':
    'prefix': 'dirac-c'
    'body': """
    #include "${1}.h"

    registerMooseObject("${2:AppName}App", ${1});

    template <>
    InputParameters
    validParams<${1}>()
    {
      InputParameters params = validParams<DiracKernel>();
      params.addClassDescription("");
      return params;
    }

    ${1}::${1}(const InputParameters & parameters)
      : DiracKernel(parameters)
    {
    }

    void
    ${1}::addPoints()
    {
    }

    Real
    ${1}::computeQpResidual()
    {
    }

    Real
    ${1}::computeQpJacobian()
    {
    }
    """
